{
  "name": "cssa",
  "version": "1.3.0",
  "description": "CSS audit tool",
  "bin": [
    "bin/cssa",
    "bin/cssa-legacy"
  ],
  "scripts": {
    "prepublish": "npm run clean && npm run test && npm run build",
    "build": "rollup -c && chmod +x bin/cssa bin/cssa-legacy",
    "postbuild": "mocha test/functional",
    "test": "mocha test/unit",
    "clean": "rm -rf bin/"
  },
  "keywords": [
    "css",
    "audit",
    "static analysis"
  ],
  "author": "Charles Hymans",
  "license": "GPL-3.0",
  "bugs": "https://github.com/c8y3/cssa/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/c8y3/cssa.git"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-register": "^6.26.0",
    "babel-root-slash-import": "^1.1.0",
    "chai": "^4.1.2",
    "fs-extra": "^5.0.0",
    "mocha": "^4.0.1",
    "rollup": "^0.52.1",
    "rollup-plugin-root-import": "^0.2.2"
  },
  "dependencies": {
    "css": "^2.2.1",
    "es6-shim": "^0.35.3",
    "stargs": "^5.0.0"
  }
}
